//User function template for C++

class Solution {
  public:
    void prank(long long a[], int n){
        // code here
        long long p;
        for(int i=0; i<n; i++)
        {
            p= (a[a[i]]%n)*n;
            a[i]+=p;
        }
        for(int i=0;i<n;i++)
            a[i] = a[i]/n;
        
        
    }
};
